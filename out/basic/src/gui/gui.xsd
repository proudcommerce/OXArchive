<?xml version="1.0" encoding="ISO-8859-15" standalone="yes"?>
<xs:schema id="OXIDGui" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="gui">
    <xs:complexType>
      <xs:sequence>

        <xs:element name="theme" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="color" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
            <xs:attribute name="id" type="xs:string" use="required" />
            <xs:attribute name="title" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>

        <xs:element name="gif" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="color" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
            <xs:attribute name="tpl" type="xs:string" use="required" />
            <xs:attribute name="file" type="xs:string" use="required" />
            <xs:attribute name="const" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>

        <xs:element name="css" minOccurs="1" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="group" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
            <xs:attribute name="tpl" type="xs:string" />
            <xs:attribute name="file" type="xs:string" />
          </xs:complexType>
        </xs:element>

      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="color">
    <xs:complexType>
      <xs:attribute name="index" type="xs:int"/>
      <xs:attribute name="color" type="xs:string" />
      <xs:attribute name="const" type="xs:string"/>
      <xs:attribute name="title" type="xs:string"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="group">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="color" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="group" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
      <xs:attribute name="title" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

</xs:schema>